{
    "name": "thunder-template",
    "private": true,
    "version": "0.0.0",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "build:stage": "tsc && vite build --mode development",
        "preview": "vite preview",
        "prettier:format": "npx prettier --write \"src/**/*.ts\"",
        "prettier:check": "npx prettier -c \"src/**/*.ts\"",
        "atlas": "node atlasBuilder.js",
        "bitmapFont": "node bitmapFontBuilder.js",
        "prepare": "husky install"
    },
    "devDependencies": {
        "free-tex-packer-core": "^0.3.4",
        "husky": "^7.0.0",
        "msdf-bmfont-xml": "^2.7.0",
        "prettier": "^2.6.1",
        "pretty-quick": "^3.1.3",
        "typescript": "^4.5.4",
        "vite": "^2.9.0",
        "vite-plugin-html": "^3.2.0",
        "vite-plugin-pwa": "^0.11.13"
    },
    "dependencies": {
        "@gamindo/thunder": "^0.1.4"
    }
}
